<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Libcu.Native</id>
    <version>0.5.0</version>
    <title>Libcu</title>
    <authors>Contoso</authors>
    <owners>Contoso</owners>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This package contains GPU structures, and should be used as a simple runtime libary.</description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2010</copyright>
    <tags>native cuda gpu runtime</tags>
    <language>en-US</language>
  </metadata>
  <files>
    <file src="_build\**\libcu.*.lib" exclude="_build\**\libcu.tinytcl.*.lib;_build\**\libcu.jimtcl.*.lib;" target="build\native\lib" />
    <file src="libcu\*.targets" target="build\native" />
    <file src="libcu\**\*.h" exclude="libcu\_*\**\*;libcu\GL\*" target="build\native\include" />
    <file src="libcu\_content\*" target="content" />
    <file src="libcu\**\*.cpp" exclude="libcu\Debug\*;libcu\Release\*;libcu\x64\**\*" target="src" />
    <file src="libcu\**\*.cu" exclude="libcu\_*\**\*" target="src" />
    <file src="libcu\**\*.h" exclude="libcu\_*\**\*;libcu\GL\*" target="src" />
  </files>
</package>
